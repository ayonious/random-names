{
  "name": "name-generator",
  "version": "0.0.0",
  "description": "Generate Random Names Based on Region",
  "keywords": [
    "react",
    "game",
    "fun",
    "randomization"
  ],
  "dependencies": {
    "@material-ui/core": "4.11.2",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-select": "^3.1.1",
    "styled-components": "5.2.1"
  },
  "scripts": {
    "cypress-headless": "cypress run --browser chrome --headless",
    "cypress-web": "cypress open",
    "start": "webpack-dev-server --open --mode development",
    "start-headless": "webpack-dev-server --mode development",
    "test-integration": "start-server-and-test start-headless http://localhost:8080 cypress-headless",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "build": "webpack --mode production",
    "unit-test": "jest --config jestconfig.json",
    "visual-test": "percy exec -t 750 -h cdn-images-1.medium.com -- cypress run",
    "visual-test-open": "percy exec -t 750 -h cdn-images-1.medium.com -- cypress open",
    "test": "yarn visual-test",
    "test-integration-and-visual": "start-server-and-test start-headless http://localhost:8080 visual-test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.12.10",
    "@babel/preset-typescript": "^7.12.7",
    "@percy/cypress": "^2.3.3",
    "@testing-library/react": "^11.2.2",
    "@types/jest": "^26.0.19",
    "@types/react": "17.0.0",
    "@types/react-dom": "17.0.0",
    "@types/react-select": "^3.1.2",
    "@types/react-test-renderer": "^17.0.0",
    "@types/styled-components": "5.1.7",
    "@types/webpack-env": "^1.16.0",
    "babel-loader": "^8.2.2",
    "cypress": "^6.2.0",
    "html-loader": "^1.3.2",
    "html-webpack-plugin": "^4.5.0",
    "husky": "^5.0.4",
    "jest": "26.6.3",
    "prettier": "2.2.1",
    "pretty-quick": "3.1.0",
    "react-test-renderer": "^17.0.1",
    "start-server-and-test": "^1.11.7",
    "ts-jest": "^26.4.4",
    "typescript": "4.1.3",
    "webpack": "^5.11.1",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.1"
  }
}
