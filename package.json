{
  "name": "name-generator",
  "version": "1.0.10",
  "description": "Generate Random Names Based on Region",
  "keywords": [
    "react",
    "game",
    "fun",
    "randomization"
  ],
  "dependencies": {
    "@material-ui/core": "4.11.0",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "styled-components": "5.1.1"
  },
  "scripts": {
    "cypress-headless": "cypress run --browser chrome --headless",
    "cypress-web": "cypress open",
    "start": "webpack-dev-server --open --mode development",
    "start-headless": "webpack-dev-server --mode development",
    "test-integration": "start-server-and-test start-headless http://localhost:8080 cypress-headless",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "build": "webpack --mode production",
    "unit-test": "jest --config jestconfig.json",
    "visual-test": "percy exec -t 750 -h cdn-images-1.medium.com -- cypress run",
    "visual-test-open": "percy exec -t 750 -h cdn-images-1.medium.com -- cypress open",
    "test": "yarn visual-test",
    "test-integration-and-visual": "start-server-and-test start-headless http://localhost:8080 visual-test"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-react": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@percy/cypress": "^2.3.1",
    "@testing-library/react": "^10.4.7",
    "@types/jest": "^26.0.5",
    "@types/react": "16.9.43",
    "@types/react-dom": "16.9.8",
    "@types/react-test-renderer": "^16.9.2",
    "@types/styled-components": "5.1.1",
    "@types/webpack-env": "^1.15.2",
    "babel-loader": "^8.1.0",
    "cypress": "^4.10.0",
    "html-loader": "^1.1.0",
    "html-webpack-plugin": "^4.3.0",
    "husky": "^4.2.5",
    "jest": "26.2.2",
    "prettier": "2.1.1",
    "pretty-quick": "2.0.1",
    "react-test-renderer": "^16.13.1",
    "start-server-and-test": "^1.11.2",
    "ts-jest": "^26.1.3",
    "typescript": "3.9.7",
    "webpack": "^4.43.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
